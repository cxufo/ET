<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.etwebservice.domain.Introduction" table="introduction">
      <id name="introId" column="introId" type="int">
      	<generator class="native"/>
      </id>
      <property name="introItem" column="introItem" type="string"></property>
      <property name="detail" column="detail" type="string"></property>
      <property name="image" column="image" type="string"></property>
  </class>
</hibernate-mapping>
