<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.etwebservice.domain.Service" table="service">
      <id name="serviceId" column="serviceId" type="int">
      	<generator class="native"/>
      </id>
      <property name="serviceName" column="serviceName" type="string"></property>
      <property name="detail" column="detail" type="string"></property>
      <property name="image" column="image" type="string"></property>
  </class>
</hibernate-mapping>
