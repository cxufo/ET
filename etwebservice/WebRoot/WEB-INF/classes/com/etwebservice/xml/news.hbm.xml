<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.etwebservice.domain.News" table="news">
      <id name="newsId" column="newsId" type="int">
      	<generator class="native"/>
      </id>
      <property name="newsTitle" column="newsTitle" type="string"></property>
      <property name="newsContent" column="newsContent" type="string"></property>
      <property name="newsTime" column="newsTime" type="string"></property>
  </class>
</hibernate-mapping>
