<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.etwebservice.domain.Contact" table="contact">
      <id name="contactId" column="contactId" type="int">
      	<generator class="native"/>
      </id>
      <property name="phoneNumber" column="phoneNumber" type="string"></property>
      <property name="qq" column="qq" type="string"></property>
      <property name="adress" column="adress" type="string"></property>
      <property name="fax" column="fax" type="string"></property>
      <property name="email" column="email" type="string"></property>
  </class>
</hibernate-mapping>
